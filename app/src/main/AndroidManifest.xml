<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.tugasbesarkotlin4.coffeedelivery">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Admin.Activity.DetailOrder">
        </activity>
        <activity android:name=".Admin.Activity.ListOrder">
        </activity>
        <activity android:name=".Customer.Activity.ConfirmOrder">
        </activity>
        <activity android:name=".Customer.Activity.Quantity">
        </activity>
        <activity android:name=".Customer.Activity.Cart">
        </activity>
        <activity android:name=".SignIn">
        </activity>
        <activity android:name=".Admin.Activity.MenuActivity">
        </activity>
        <activity android:name=".ResetPasswordActivity">
        </activity>
        <activity android:name=".Customer.Activity.DasboardCustomer">
        </activity>
        <activity android:name=".Admin.Activity.DasboardAdmin">
        </activity>
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>